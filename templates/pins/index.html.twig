{% extends 'base.html.twig' %}

{% block title %}panterest{% endblock %}

{% block body %}
    <h1>{{ pluralize(pins|length, "pin") }}</h1>
    {% for pin in pins %}
        <article>
            <h2>{{pin.title}}</h2>
            <p>{{pin.description}}</p>
        </article>
    {% endfor %}
{% endblock %}
